# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'UsingPods' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for UsingPods
  # Those pods are being used in our project to illustrate using
  # Third party libraries
 pod 'Alamofire'
 pod 'SnapKit'
 pod 'Kingfisher'

  target 'UsingPodsTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
